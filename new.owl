#################################################################

###  http://metadataschema.pht.de#CPUUsageReportEvent
:CPUUsageReportEvent rdf:type owl:Class ;
                     rdfs:subClassOf :StationRelatedTripEvent .


###  http://metadataschema.pht.de#DataInterface
:DataInterface rdf:type owl:Class ;
               rdfs:subClassOf :PHTConcept ;
               rdfs:comment "Describes the Interface at a Station through which data can be accessed."@en ,
                            "Describes the actual Interface. The Form of the Interface is described by the connected DataInterfaceShape object."@en ;
               rdfs:seeAlso :DataInterfaceShape .


###  http://metadataschema.pht.de#DataInterfaceShape
:DataInterfaceShape rdf:type owl:Class ;
                    rdfs:subClassOf :PHTConcept ;
                    rdfs:comment "Describes the shape of an Interface at a station through which data can be accessed."@en .


###  http://metadataschema.pht.de#Dataset
:Dataset rdf:type owl:Class ;
         rdfs:subClassOf :PHTConcept ;
         rdfs:comment "A data set of a station to which the station provides access through an interface for trains."@en .


###  http://metadataschema.pht.de#DockerMaxSizeConstrain
:DockerMaxSizeConstrain rdf:type owl:Class ;
                        rdfs:subClassOf :TrainConstrain .


###  http://metadataschema.pht.de#EstimatedComputationalPowerConstrain
:EstimatedComputationalPowerConstrain rdf:type owl:Class ;
                                      rdfs:subClassOf :TrainConstrain .


###  http://metadataschema.pht.de#ExecutionEnvironment
:ExecutionEnvironment rdf:type owl:Class ;
                      rdfs:subClassOf :PHTConcept ;
                      rdfs:comment "Describes the environment of a station in which trains can be executed."@en .


###  http://metadataschema.pht.de#FinishedRunningAtStationEvent
:FinishedRunningAtStationEvent rdf:type owl:Class ;
                               rdfs:subClassOf :StationRelatedTripEvent .


###  http://metadataschema.pht.de#FinishedTransmissionEvent
:FinishedTransmissionEvent rdf:type owl:Class ;
                           rdfs:subClassOf :StationRelatedTripEvent .


###  http://metadataschema.pht.de#Format
:Format rdf:type owl:Class ;
        rdfs:subClassOf :PHTConcept ;
        rdfs:comment "Class describing the Format of a Train."@en ,
                     "Multiple Instances of this classes are shipped. It should not be necessary to instantiate this class, since then we would have non standardised Formats."@en .


###  http://metadataschema.pht.de#InternetAccessConstrain
:InternetAccessConstrain rdf:type owl:Class ;
                         rdfs:subClassOf :TrainConstrain ;
                         rdfs:comment "Describes a constrain which states if a train needs access to the internet." .


###  http://metadataschema.pht.de#LocalFileInterface
:LocalFileInterface rdf:type owl:Class ;
                    rdfs:subClassOf :DataInterface ;
                    owl:disjointWith :NetworkInterface ;
                    rdfs:comment "Describes an Interface which is made available to the train via a local file path."@en .


###  http://metadataschema.pht.de#MemoryUsageReportEvent
:MemoryUsageReportEvent rdf:type owl:Class ;
                        rdfs:subClassOf :StationRelatedTripEvent .


###  http://metadataschema.pht.de#NetworkInterface
:NetworkInterface rdf:type owl:Class ;
                  rdfs:subClassOf :DataInterface ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :networkInterfaceAdress ;
                                    owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onDataRange xsd:string
                                  ] ;
                  rdfs:comment "A Data Interface which is accessed over the network in the Station"@en .


###  http://metadataschema.pht.de#PHTConcept
:PHTConcept rdf:type owl:Class ;
            rdfs:subClassOf owl:Thing .


###  http://metadataschema.pht.de#PlannedHalt
:PlannedHalt rdf:type owl:Class ;
             rdfs:subClassOf :PHTConcept ;
             rdfs:comment "A single step in the Trip of the Train. Describes a plan of a visit at a station. Can be fulfilled by a visit." .


###  http://metadataschema.pht.de#PlannedParallelHalt
:PlannedParallelHalt rdf:type owl:Class ;
                     rdfs:subClassOf :PlannedHalt ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :consistsOfHalt ;
                                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass :PlannedSingleHalt
                                     ] ;
                     owl:disjointWith :PlannedSingleHalt .


###  http://metadataschema.pht.de#PlannedSingleHalt
:PlannedSingleHalt rdf:type owl:Class ;
                   rdfs:subClassOf :PlannedHalt .


###  http://metadataschema.pht.de#StartedRunningEvent
:StartedRunningEvent rdf:type owl:Class ;
                     rdfs:subClassOf :StationRelatedTripEvent .


###  http://metadataschema.pht.de#StartedTransmissionEvent
:StartedTransmissionEvent rdf:type owl:Class ;
                          rdfs:subClassOf :TripEvent .


###  http://metadataschema.pht.de#State
:State rdf:type owl:Class ;
       rdfs:subClassOf :PHTConcept ;
       rdfs:comment "This class describes a state of a train. A state of a train captures the state of the filesystem. If the filesystem changes, the state is not the same anymore. Therefore, an execution of a train at at station can be considered as a transformation from one state to another." .


###  http://metadataschema.pht.de#Station
:Station rdf:type owl:Class ;
         rdfs:subClassOf :PHTConcept ,
                         :UserOwned .


###  http://metadataschema.pht.de#StationErrorEvent
:StationErrorEvent rdf:type owl:Class ;
                   rdfs:subClassOf :StationRelatedTripEvent .


###  http://metadataschema.pht.de#StationLogEvent
:StationLogEvent rdf:type owl:Class ;
                 rdfs:subClassOf :StationRelatedTripEvent .


###  http://metadataschema.pht.de#StationRejectedEvent
:StationRejectedEvent rdf:type owl:Class ;
                      rdfs:subClassOf :StationRelatedTripEvent .


###  http://metadataschema.pht.de#StationRelatedTripEvent
:StationRelatedTripEvent rdf:type owl:Class ;
                         rdfs:subClassOf :TripEvent ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :eventEmittedByStation ;
                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass :Station
                                         ] .


###  http://metadataschema.pht.de#TabularDataInterfaceShape
:TabularDataInterfaceShape rdf:type owl:Class ;
                           rdfs:subClassOf :DataInterfaceShape .


###  http://metadataschema.pht.de#Train
:Train rdf:type owl:Class ;
       rdfs:subClassOf :PHTConcept ,
                       :UserOwned .


###  http://metadataschema.pht.de#TrainConstrain
:TrainConstrain rdf:type owl:Class ;
                rdfs:subClassOf :PHTConcept .


###  http://metadataschema.pht.de#TrainIstantiatedEvent
:TrainIstantiatedEvent rdf:type owl:Class ;
                       rdfs:subClassOf :TripEvent .


###  http://metadataschema.pht.de#TrainStore
:TrainStore rdf:type owl:Class ;
            rdfs:subClassOf :PHTConcept ;
            rdfs:comment "An abstract class describing an entity which stores trains and provides interfaces for retrieving those." .


###  http://metadataschema.pht.de#TrainStoreInterface
:TrainStoreInterface rdf:type owl:Class ;
                     rdfs:subClassOf :PHTConcept ;
                     rdfs:comment "An interface of a Train Store to retrieve Trains."@en .


###  http://metadataschema.pht.de#Trip
:Trip rdf:type owl:Class ;
      rdfs:subClassOf :PHTConcept ;
      rdfs:comment "An execution of a Train. Consists of multiple Visits of stations" .


###  http://metadataschema.pht.de#TripEvent
:TripEvent rdf:type owl:Class ;
           rdfs:subClassOf :PHTConcept ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :eventEmittedDuring ;
                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :Trip
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :eventTimestamp ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onDataRange xsd:dateTimeStamp
                           ] ;
           rdfs:comment "An event which is emitted during a Trip of a Train. Can contain various information."@en .


###  http://metadataschema.pht.de#UserOwned
:UserOwned rdf:type owl:Class ;
           rdfs:subClassOf :PHTConcept .


###  http://metadataschema.pht.de#Visit
:Visit rdf:type owl:Class ;
       rdfs:subClassOf :PHTConcept ;
       rdfs:comment "Describes a single visit of a train during a trip at a station." .


