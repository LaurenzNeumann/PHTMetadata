#################################################################
#    Object Properties
#################################################################

###  http://metadataschema.pht.de#accessThorughInterface
:accessThorughInterface rdf:type owl:ObjectProperty ;
                        owl:inverseOf :providesDataAccessTo ;
                        rdfs:domain :Dataset ;
                        rdfs:range :DataInterface ;
                        rdfs:comment "Connects a Data set to an interface through which it can be accessed."@en .


###  http://metadataschema.pht.de#associatedWithStore
:associatedWithStore rdf:type owl:ObjectProperty ;
                     rdfs:domain :Station ;
                     rdfs:range :TrainStore .


###  http://metadataschema.pht.de#byTrain
:byTrain rdf:type owl:ObjectProperty ;
         rdfs:domain :Trip ;
         rdfs:range :Train ;
         rdfs:comment "Connects the Trip (Execution) of a Train to the Train itself."@en .


###  http://metadataschema.pht.de#consistsOfHalt
:consistsOfHalt rdf:type owl:ObjectProperty ;
                rdfs:domain :PlannedParallelHalt ;
                rdfs:range :PlannedSingleHalt .


###  http://metadataschema.pht.de#emittedEvent
:emittedEvent rdf:type owl:ObjectProperty ;
              owl:inverseOf :eventEmittedDuring .


###  http://metadataschema.pht.de#environmentSupportsFormat
:environmentSupportsFormat rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf owl:topObjectProperty ;
                           rdfs:comment "Describes that an Environment (S) supports a Format of a Train (O)"@en .


###  http://metadataschema.pht.de#eventEmittedByEntity
:eventEmittedByEntity rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdfs:comment "Describes that an TripEvent was emitted by an Entity of the PHT architecture."@en .


###  http://metadataschema.pht.de#eventEmittedByStation
:eventEmittedByStation rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :eventEmittedByEntity ;
                       rdfs:domain :StationRelatedTripEvent ;
                       rdfs:range :Station .


###  http://metadataschema.pht.de#eventEmittedDuring
:eventEmittedDuring rdf:type owl:ObjectProperty ;
                    rdfs:domain :TripEvent ;
                    rdfs:range :Trip .


###  http://metadataschema.pht.de#fulfilledBy
:fulfilledBy rdf:type owl:ObjectProperty ;
             rdfs:domain :PlannedSingleHalt ;
             rdfs:range :Visit ;
             rdfs:comment "Describes that a PlannedHalt was fulfilled by a visit."@en .


###  http://metadataschema.pht.de#haltAtStation
:haltAtStation rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :PlannedSingleHalt ;
               rdfs:range :Station .


###  http://metadataschema.pht.de#hasInterfaceShape
:hasInterfaceShape rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain :DataInterface ;
                   rdfs:range :DataInterfaceShape ;
                   rdfs:comment "Connects a DataInterface to a Shape which describes it."@en .


###  http://metadataschema.pht.de#isStoredByStoreType
:isStoredByStoreType rdf:type owl:ObjectProperty ;
                     owl:inverseOf :storesFormat ;
                     rdfs:domain :Format ;
                     rdfs:range :TrainStore ;
                     rdfs:comment "Describes what kind of stores can store this Format."@en .


###  http://metadataschema.pht.de#providesDataAccessTo
:providesDataAccessTo rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdfs:domain :DataInterface ;
                      rdfs:range :Dataset .


###  http://metadataschema.pht.de#responsibleOrganization
:responsibleOrganization rdf:type owl:ObjectProperty ;
                         rdfs:domain :UserOwned ;
                         rdfs:range <http://xmlns.com/foaf/0.1/Organization> .


###  http://metadataschema.pht.de#stateFormat
:stateFormat rdf:type owl:ObjectProperty ;
             rdfs:domain :State ;
             rdfs:range :Format ;
             rdfs:comment "Describes the Format of a State."@en .


###  http://metadataschema.pht.de#storeInterfaceProvidesFormat
:storeInterfaceProvidesFormat rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf owl:topObjectProperty ;
                              rdfs:domain :TrainStoreInterface ;
                              rdfs:range :Format ;
                              rdfs:comment "Describes that a TrainStore interface (S) provides a special Format of Trains (O)."@en .


###  http://metadataschema.pht.de#storesFormat
:storesFormat rdf:type owl:ObjectProperty .


###  http://metadataschema.pht.de#yieldedState
:yieldedState rdf:type owl:ObjectProperty ;
              rdfs:domain :Visit ;
              rdfs:range :State ;
              rdfs:comment "Connects a Visit at a Station by a Train to the new State of the Train in the Trip yielded by this Visit."@en .


